{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": "container",
    "height": 350,
    "title": "Import/Export",
    "data": {
      "url": "https://raw.githubusercontent.com/Reachsangmonash/Fit3179_Vis2/main/Countries%203.csv"
    },
    "repeat": {"layer": ["Import", "Export"]},
    "spec": {
      "mark": "bar",
      "encoding": {
        "x": {
          "field": "Country Name",
          "type": "nominal",
          "stack": false
        },
        "y": {
          "aggregate":"sum",
          "field": {"repeat": "layer"},
          "type": "quantitative",
          "title": "Import/Exmport",
          "stack": false
        },
        "color": {"datum": {"repeat": "layer"}, "title": "Gross"},
        "xOffset": {"datum": {"repeat": "layer"}},
        "tooltip":[
            {"field": "Country Name",
            "type": "nominal",
                "title": "Country"},
        {"field": {"repeat": "layer"},
    "type": "quantitative"}
        ]
          
      }
    },
    "config": {
      "mark": {"invalid": null}
    },
  }
  